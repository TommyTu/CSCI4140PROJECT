			    <div class="loading" id="loading">
		  			<div class="loading-box"><i class="ion-ios-loop"></i></div>
			    </div>
			    <div class="select-list" id="select-list">
			    </div>
			  </div>
			  <div class="col-md-4">
			  	<div class="export">
	    			<button class="clickable" id="export-btn"><span><i class="ion-ios-download-outline"></i></span>  Export</button>
			  	</div>
			  	<div class="search">
    				<div class="search-bar input-box">
	    				<input type="text" name="search" placeholder="Search" id="search-input">
	    				<i class="ion-ios-search"></i>
    				</div>
	    		</div>
	    		<div class="search-list" id="search-list">
	    		</div>
	    	</div>
    	</div>
    </div>

    <!-- Modal -->
		<div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="loginModalLabel" aria-hidden="true">
		  <div class="modal-dialog" role="document">
		    <div class="modal-content">
		      <div class="modal-header">
		        <h5 class="modal-title" id="loginModalLabel">Login with CWEM</h5>
		        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
		          <span aria-hidden="true"><i class="ion-ios-close-outline"></i></span>
		        </button>
		      </div>
		      <div class="modal-body">
		        <!-- <form method="POST" action="/login" id="login-form"> -->
		        	<div id="login-warning">Login fail! Please try it again.</div>
		        	<div class="login-input input-box">
		    				<i class="ion-ios-person-outline" ></i>
		    				<input id="sid-input" type="text" name="sid" placeholder="11550xxxxx">
	    				</div>
	    				<div class="login-input input-box">
		    				<i class="ion-ios-keypad-outline" ></i>
		    				<input id="pwd-input" type="password" name="pwd" placeholder="Your CWEM Password">
	    				</div>
		        <!-- </form> -->
		      </div>
		      <div class="modal-footer">
		        <button type="submit" id="login-btn" class="btn btn-cute">Login</button>
		      </div>
		    </div>
		  </div>
		</div>

		<template id="class-item-tmpl">
			<div class="class-item">
				<div class="class-info">
				</div>
			</div>
		</template>

		<template id="search-item-tmpl">
			<div class="search-item clickable">
				<div class="search-item-info">
				</div>
				<a target="_blank" class="search-item-btn">
					<i class="ion-information-circled"></i>
				</a>
			</div>
		</template>

		<template id="select-item-tmpl">
			<div class="select-item">
    		<div class="select-item-tooltip">
    			<!-- select-item-lec -->
    		</div>
    		<div class="select-item-show">
    			<a target="_blank" class="select-item-info"><i class="ion-information-circled"></i></a>
  				<div class="select-item-title"></div>
    			<div class="select-item-btn">
    				<i class="ion-ios-trash-outline delete-btn clickable-btn"></i>
	    			<i class="ion-ios-eye hide-btn clickable-btn"></i>
    			</div>
    		</div>
    	</div>
		</template>

		<template id="select-item-comp-tmpl">
			<div class="flex"> 
				<div class="select-item-comp-title" style="margin-right: 7px"></div>
				<div class="select-item-comp-sec">
					<!-- select-item-sec-tmpl -->
				</div>
  		</div>
		</template>

  	<template id="select-item-sec-tmpl">
